{
  "plugins": {
    "allow": [
      "whatsapp"
    ]
  },
  "web": {
    "enabled": true
  },
  "gateway": {
    "mode": "local",
    "port": 18789,
    "controlUi": {
      "dangerouslyAllowHostHeaderOriginFallback": true,
      "dangerouslyDisableDeviceAuth": true
    }
  },
  "agents": {
    "defaults": {
      "model": "google/gemini-2.5-flash",
      "workspace": "./workspace"
    }
  },
  "tools": {
    "web": {
      "search": {
        "gemini": {
          "model": "gemini-2.5-flash"
        }
      }
    }
  },
  "channels": {
    "whatsapp": {
      "accounts": {
        "primary": {
          "enabled": true,
          "dmPolicy": "open",
          "allowFrom": [
            "*"
          ]
        }
      }
    }
  }
}
